#define jbyte  signed<1>
#define jshort signed<2>
#define jint   signed<4>
#define jlong  signed<8>
#define jchar  unsigned<2>
#define HASH_SIZE 10000
declare database AsyncEventDb;
class MyClass {
    autoid[HASH_SIZE];
    jint key;
    jint value;
    event<new> NewEvent;
    event<delete> DeleteEvent;
    event<delete_all> DeleteAllEvent;
    event<checkpoint> CheckPointEvent;
    event<value update> FieldUpdateEvent;
    tree<key> key_index;
};
