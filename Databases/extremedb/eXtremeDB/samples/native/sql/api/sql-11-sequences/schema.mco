#define int1      signed<1>
#define int2      signed<2>
#define int4      signed<4>
#define int8      signed<8>
#define uint8     unsigned<8>
#define uint4     unsigned<4>
#define uint2     unsigned<2>
#define uint1     unsigned<1>

#define sid_t     uint4

declare database  bbfisearch;


class BondHistory
{
    sid_t   sid;
    string cusip;

    sequence<uint4 asc> dates;
    sequence<float> prices;
    sequence<char<4>> marks;

    unique tree<cusip> by_cusip;
};
